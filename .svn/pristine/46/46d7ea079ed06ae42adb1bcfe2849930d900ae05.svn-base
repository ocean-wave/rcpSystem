package cn.com.cdboost.collect.dto.response;

import java.math.BigDecimal;

/**
 * 提现页面需要返回的信息
 */
public class WithdrawCashInfo {
    /**
     * 当前余额
     */
    private BigDecimal remainAmount;

    /**
     * 可提现金额
     */
    private BigDecimal withdrawCashAmount;

    /**
     * 联系电话
     */
    private String contactTelphone;

    /**
     * 是否存在其他可提现账户 1表示存在，0 不存在
     */
    private Integer hasAccount = 0;

    /**
     * 微信openId或者支付宝userId
     */
    private String userId;

    public BigDecimal getRemainAmount() {
        return remainAmount;
    }

    public void setRemainAmount(BigDecimal remainAmount) {
        this.remainAmount = remainAmount;
    }

    public BigDecimal getWithdrawCashAmount() {
        return withdrawCashAmount;
    }

    public void setWithdrawCashAmount(BigDecimal withdrawCashAmount) {
        this.withdrawCashAmount = withdrawCashAmount;
    }

    public String getContactTelphone() {
        return contactTelphone;
    }

    public void setContactTelphone(String contactTelphone) {
        this.contactTelphone = contactTelphone;
    }

    public Integer getHasAccount() {
        return hasAccount;
    }

    public void setHasAccount(Integer hasAccount) {
        this.hasAccount = hasAccount;
    }

    public String getUserId() {
        return userId;
    }

    public void setUserId(String userId) {
        this.userId = userId;
    }
}
