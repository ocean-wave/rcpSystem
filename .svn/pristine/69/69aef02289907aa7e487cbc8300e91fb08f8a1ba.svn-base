public enum ResultCode {
    Success(1, "成功"),
    Fail(0, "失败"),
    ParamError(100003, "参数不合法"),
    MONEY_ERROR(200001,"金额不正确");

    private int value;

    private String desc;

    ResultCode(int value, String desc) {
        this.value = value;
        this.desc = desc;
    }

    public int getValue() {
        return value;
    }

    public String getDesc() {
        return desc;
    }

}
