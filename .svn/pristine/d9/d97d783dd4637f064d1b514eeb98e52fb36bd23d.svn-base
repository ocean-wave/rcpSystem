package cn.com.cdboost.collect.dto.param;

import org.hibernate.validator.constraints.NotBlank;

import javax.validation.constraints.NotNull;

/**
 * @author zc
 * @desc
 * @create 2017-12-14 16:41
 **/
public class JzqAddParam {

    /**
     * 集中器编号
     */
    @NotBlank(message = "deviceNo不能为空")
    private String deviceNo;

    /**
     * 组织编号
     */
    @NotNull(message = "orgNo不能为null")
    private Long orgNo;

    /**
     * 设备厂家
     */
    private String deviceFactory;

    /**
     * 安装日期
     */
    @NotBlank(message = "installDate不能为空")
    private String installDate;

    /**
     * 主站IP
     */
    @NotBlank(message = "webSiteIp不能为空")
    private String webSiteIp;

    /**
     * 主站端口
     */
    @NotBlank(message = "webSitePort不能为空")
    private String webSitePort;

    /**
     * 通道APN
     */
    @NotBlank(message = "apn不能为空")
    private String apn;

    /**
     * 安装位置
     */
    private String installAddr;

    /**
     * 备注
     */
    private String remark;

    public String getDeviceNo() {
        return deviceNo;
    }

    public void setDeviceNo(String deviceNo) {
        this.deviceNo = deviceNo;
    }

    public Long getOrgNo() {
        return orgNo;
    }

    public void setOrgNo(Long orgNo) {
        this.orgNo = orgNo;
    }

    public String getDeviceFactory() {
        return deviceFactory;
    }

    public void setDeviceFactory(String deviceFactory) {
        this.deviceFactory = deviceFactory;
    }

    public String getInstallDate() {
        return installDate;
    }

    public void setInstallDate(String installDate) {
        this.installDate = installDate;
    }

    public String getWebSiteIp() {
        return webSiteIp;
    }

    public void setWebSiteIp(String webSiteIp) {
        this.webSiteIp = webSiteIp;
    }

    public String getWebSitePort() {
        return webSitePort;
    }

    public void setWebSitePort(String webSitePort) {
        this.webSitePort = webSitePort;
    }

    public String getApn() {
        return apn;
    }

    public void setApn(String apn) {
        this.apn = apn;
    }

    public String getInstallAddr() {
        return installAddr;
    }

    public void setInstallAddr(String installAddr) {
        this.installAddr = installAddr;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }
}
