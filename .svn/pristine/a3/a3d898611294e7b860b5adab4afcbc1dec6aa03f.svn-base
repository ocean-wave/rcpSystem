package cn.com.cdboost.collect.constant;

/**
 * redis key常量前缀
 */
public class RedisKeyConstant {
    // 组织
    public static final String ORG_PREFIX = "org:";
    // 记录当前组织的父组织，value=List<Long>
    public static final String ORG_PARENT_PREFIX = "org:parent:";
    // 组织是否存在子节点 value=true 或 false(这个接口暂时没用到)
    public static final String ORG_EXIST_CHILD_PREFIX = "org:exist:child:";
    // 某个组织所在的整个组织树列表
    public static final String ORG_TREE_PREFIX = "org:tree:";
    // 某个组织以及他的孩子节点列表
    public static final String ORG_CHILD_PREFIX = "org:child:";
    // 用户拥有的组织权限 key=userId，value=List<Long> 组织orgNo列表
    public static final String USER_ORGS_PREFIX = "user:orgs:";
    // 客户档案
    public static final String CUSTOMER_PREFIX = "customer:";
    // 设备
    public static final String DEVICE_PREFIX = "device:";
    // 客户设备关联表信息key=cno
    public static final String DEVICE_MAP_PREFIX = "device:map:";
    // 设备关系
    public static final String DEVICE_RELATION_PREFIX = "relation:";
    // 设备是否存在子节点 value=true 或 false
    public static final String  DEVICE_CHILD_PREFIX = "device:child:";
    // 设备挂靠的孩子节点（所有孩子节点）
    public static final String DEVICE_RELATION_CHILD_PREFIX = "relation:child:";
    // 设备状态
    public static final String DEVICE_STATE_PREFIX = "device:state:";
    // 菜单
    public static final String MENU_PREFIX = "menu:";

    // 充电桩充电相关
    // 活动页面充值
    public static final String ACTIVITY_CHARGE_PREFIX = "activity:charge:";
    // 月卡充值
    public static final String MONTH_CARD_CHARGE_PREFIX = "month:card:charge:";
    // 充电设备充值
    public static final String CHARGE_ELEC_PREFIX = "charge:elec:";
    // 微信提现
    public static final String WECHAT_CASH_PREFIX = "wechat:cash:";
    // 支付宝提现
    public static final String ALIPAY_CASH_PREFIX = "alipay:cash:";

}
