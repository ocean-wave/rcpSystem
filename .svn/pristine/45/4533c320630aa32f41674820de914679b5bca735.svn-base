            <version>3.2.0</version>
        </dependency>
        <!--rabbitmq-->
        <dependency>
            <groupId>com.rabbitmq</groupId>
            <artifactId>amqp-client</artifactId>
            <version>4.1.0</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.amqp</groupId>
            <artifactId>spring-rabbit</artifactId>
            <version>1.4.5.RELEASE</version>
        </dependency>
        <dependency>
            <groupId>cn.com.cdboost</groupId>
            <artifactId>sensorDubbo</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
    </dependencies>
    <profiles>
        <!-- 本地开发环境 -->
        <profile>
            <id>dev-250</id>
            <properties>
                <env>dev-250</env>
            </properties>
        </profile>

        <!-- 123中天测试环境 -->
        <profile>
            <id>zt-123</id>
            <properties>
                <env>zt-123</env>
            </properties>
        </profile>

        <!-- 250测试环境 -->
        <profile>
            <id>test-250</id>
            <properties>
                <env>test-250</env>
            </properties>
        </profile>

        <!-- 250中天环境 -->
        <profile>
            <id>zt-250</id>
            <properties>
                <env>zt-250</env>
            </properties>
            <activation>
                <!-- 默认激活这个配置 -->
                <activeByDefault>true</activeByDefault>
            </activation>
        </profile>

        <!-- 251压力测试环境 -->
        <profile>
            <id>yl-251</id>
            <properties>
                <env>yl-251</env>
            </properties>
        </profile>

        <!-- 251客户演示环境 -->
        <profile>
            <id>ys-251</id>
            <properties>
                <env>ys-251</env>
            </properties>
        </profile>

        <!-- 正式发布版 -->
        <profile>
            <id>release</id>
            <properties>
                <env>release</env>
            </properties>
            <build>
                <finalName>rcpSystem</finalName>
                <filters>
                    <filter>src/main/resources/config/system-${env}.properties</filter>
                </filters>
                <resources>
                    <resource>
                        <directory>src/main/resources</directory>
                        <filtering>true</filtering>
                        <excludes>
                            <exclude>config/*</exclude>
                        </excludes>
                    </resource>
                </resources>
            </build>
        </profile>
    </profiles>

    <build>
        <finalName>rcpSystem</finalName>
        <filters>
            <filter>src/main/resources/config/system-${env}.properties</filter>
            <filter>src/main/resources/config/db-${env}.properties</filter>
        </filters>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <filtering>true</filtering>
                <excludes>
                    <exclude>config/*</exclude>
                    <!--<exclude>db.properties</exclude>-->
                </excludes>
            </resource>
        </resources>
    </build>
</project>
