    private Date changeTime;  // 更换时间
    private String changeRemark;  // 更换原因
    private String meterType;  // 设备类型 0其他 1 -09电表 2-13表
    private String newMeterType;  // 设备类型 0其他 1 -09电表 2-13表
    private int localControl;  // // 1 本地费控 2 远程费控 0 其他

    /**
     * 旧表的系统充值总额
     */
    private BigDecimal totalAmount;

    /**
     * 旧表的系统购电次数
     */
    private int payCount;

    /**
     * 新表的系统充值总额，默认都是零
     */
    private BigDecimal newTotalAmount = BigDecimal.ZERO;

    /**
     * 新表的系统购电次数，默认都是零
     */
    private int newPayCount = 0;

    public String getCustomerNo() {
        return customerNo;
    }

    public void setCustomerNo(String customerNo) {
        this.customerNo = customerNo;
    }

    public String getMeterNo() {
        return meterNo;
    }

    public void setMeterNo(String meterNo) {
        this.meterNo = meterNo;
    }

    public String getNewMeterNo() {
        return newMeterNo;
    }

    public void setNewMeterNo(String newMeterNo) {
        this.newMeterNo = newMeterNo;
    }

    public String getDeviceType() {
        return deviceType;
    }

    public void setDeviceType(String deviceType) {
        this.deviceType = deviceType;
    }

    public BigDecimal getPower() {
        return power;
    }

    public void setPower(BigDecimal power) {
        this.power = power;
    }

    public BigDecimal getNewPower() {
        return newPower;
    }

    public void setNewPower(BigDecimal newPower) {
        this.newPower = newPower;
    }

    public BigDecimal getRemainAmount() {
        return remainAmount;
    }

    public void setRemainAmount(BigDecimal remainAmount) {
        this.remainAmount = remainAmount;
    }

    public BigDecimal getNewRemainAmount() {
        return newRemainAmount;
    }

