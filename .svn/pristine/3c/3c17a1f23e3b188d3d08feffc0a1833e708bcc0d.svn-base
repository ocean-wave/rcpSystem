package cn.com.cdboost.collect.dto;

import java.math.BigDecimal;
import java.util.List;

/**
 * 微信网页上关于该设备相关的一些信息
 */
public class DeviceInfo4WeChat {
    /**
     * 集中器编号
     */
    private String 	jzqCno;

    /**
     * 用户姓名
     */
    private String 	userName;

    /**
     * 用户唯一标识
     */
    private String customerNo;

    /**
     * 表计户号
     */
    private String meterUserNo;

    /**
     * 费控类型 1本地费控,2远程费控
     */
    private Integer localControl;

    /**
     * 组织编号
     */
    private Long orgNo;

    /**
     * 用户地址
     */
    private String customerAddr;

    /**
     * 剩余金额
     */
    private String remainAmount;

    /**
     * 剩余金额采集时间
     */
    private String amountCollectTime;

    /**
     * 本月用电量
     */
    private BigDecimal currentMonthSumData;

    /**
     * 通断状态 0断开 1连通
     */
    private int isOn;

    /**
     * 设备支付状态 0表示存在一条缴费记录正在处理中  1表示其他状态
     */
    private int meterPayStatus;

    /**
     * 半年统计数据，y轴数据
     */
    private List<BigDecimal> monthSumData;

    /**
     * x轴数据
     */
    private List<String> yearMonthData;

    public String getJzqCno() {
        return jzqCno;
    }

    public void setJzqCno(String jzqCno) {
        this.jzqCno = jzqCno;
    }

    public String getUserName() {
        return userName;
    }

    public void setUserName(String userName) {
        this.userName = userName;
    }

    public String getCustomerNo() {
        return customerNo;
    }

    public void setCustomerNo(String customerNo) {
        this.customerNo = customerNo;
    }

    public String getMeterUserNo() {
        return meterUserNo;
    }

    public void setMeterUserNo(String meterUserNo) {
        this.meterUserNo = meterUserNo;
    }

    public Long getOrgNo() {
        return orgNo;
    }

    public void setOrgNo(Long orgNo) {
        this.orgNo = orgNo;
    }

    public String getCustomerAddr() {
        return customerAddr;
    }

    public void setCustomerAddr(String customerAddr) {
        this.customerAddr = customerAddr;
    }

    public String getRemainAmount() {
        return remainAmount;
    }

    public void setRemainAmount(String remainAmount) {
        this.remainAmount = remainAmount;
    }

    public String getAmountCollectTime() {
        return amountCollectTime;
    }

    public void setAmountCollectTime(String amountCollectTime) {
        this.amountCollectTime = amountCollectTime;
    }

    public BigDecimal getCurrentMonthSumData() {
        return currentMonthSumData;
    }

    public void setCurrentMonthSumData(BigDecimal currentMonthSumData) {
        this.currentMonthSumData = currentMonthSumData;
    }

    public int getIsOn() {
        return isOn;
    }

    public void setIsOn(int isOn) {
        this.isOn = isOn;
    }

    public int getMeterPayStatus() {
        return meterPayStatus;
    }

    public void setMeterPayStatus(int meterPayStatus) {
        this.meterPayStatus = meterPayStatus;
    }

    public List<BigDecimal> getMonthSumData() {
        return monthSumData;
    }

    public void setMonthSumData(List<BigDecimal> monthSumData) {
        this.monthSumData = monthSumData;
    }

    public List<String> getYearMonthData() {
        return yearMonthData;
    }

    public void setYearMonthData(List<String> yearMonthData) {
        this.yearMonthData = yearMonthData;
    }

    public Integer getLocalControl() {
        return localControl;
    }

    public void setLocalControl(Integer localControl) {
        this.localControl = localControl;
    }
}
